@model IEnumerable<SubclassBinding.Models.Product>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <title>Index</title>
</head>
<body>
    <div>
    @using (Html.BeginForm())
    {
